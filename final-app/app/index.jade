doctype html
//[if IE 9]><html class="lt-ie10" lang="en" > <![endif]
html(class='no-js', lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title Ecode
    link(rel='stylesheet', href='/css/normalize.css')
    link(href='//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css', rel='stylesheet')
    link(rel='stylesheet', href='/css/foundation.css')
    link(rel='stylesheet', href='/css/style2.css')
    link(rel='stylesheet', href='/css/animate.css')
    link(rel='stylesheet', href='/css/app.css')
    script(src='/js/vendor/modernizr.js')

body
  #slideshow
    ul.cb-slideshow
      li
        span Image 1
        div
          h4 JQuery
      li
        span Image 2
        div
          h4 HTML/Jade
      li
        span Image 3
        div
          h4 Javascript
      li
        span Image 4
        div
          h4 CSS
      li
        span Image 5
        div
          h4 Lo-Dash
      li
        span Image 6
        div
          h4 Git
  .nav.top-bar(data-topbar)
    section.top-bar-section
      ul.text-area
        li.name
          a#ecode E_Code
            i(class='fa fa-info-circle fa-1x')
      ul.left
        li.active
          a#home Home
            i(class='fa fa-home fa-1x')
      ul.right
             li.has-dropdown.full-width.not-click
                a(href='#')#jquery Jquery
                  i(class='fa fa-bars fa-1x')
                ul.dropdown
                  li.list
                    a.search#searchJQ Search
                    a.quiz#quizJQ Quiz
                    a.about#aboutJQ About Jquery
                    a.link#linkJQ Links Jquery
              li.has-dropdown.not-click
                a(href='#')#html-jade HTML/Jade
                  i(class='fa fa-bars fa-1x')
                ul.dropdown
                  li.list
                    a.search#searchHJ Search
                    a.quiz#quizHJ Quiz
                    a.about#aboutHJ About HTML/Jade
                    a.link#linkHJ Links HTML/Jade
              li.has-dropdown.not-click
                a(href='#')#java Javascript
                  i(class='fa fa-bars fa-1x')
                ul.dropdown
                  li.list
                    a.search#searchJS Search
                    a.quiz#quizJS Quiz
                    a.about#aboutJS About JS
                    a.link#linkJS Links JS
              li.has-dropdown.not-click
                a(href='#')#css CSS
                  i(class='fa fa-bars fa-1x')
                ul.dropdown
                  li.list
                    a.search#searchCSS Search
                    a.quiz#quizCSS Quiz
                    a.about#aboutCSS About CSS
                    a.link#linkCSS Links CSS
              li.has-dropdown.not-click
                a(href='#')#lodash LoDash
                  i(class='fa fa-bars fa-1x')
                ul.dropdown
                  li.list
                    a.search#searchLD Search
                    a.quiz#quizLD Quiz
                    a.about#aboutLD About LoDash
                    a.link#linkLD Links LoDash
              li.has-dropdown.not-click
                a(href='#')#git Git
                  i(class='fa fa-bars fa-1x')
                ul.dropdown
                  li.list
                    a.search#searchG Search
                    a.quiz#quizG Quiz
                    a.about#aboutG About Git
                    a.link#linkG Links Git
      .row
        .small-12.columns
          .panel#about(style='display: none')
            h1 About Us
            p E_Code was created by Lauren Bowser and Mitchell Hollander. 
            p We wanted to create a site that helped future developers learn how to code in a easy, less stressful manner.
            .row
              .small-6.columns
                #lauren.front
                  img#bbg(src='/media/bbg.gif')
                  #content1.back.hidden
                      a.links(href='https://github.com/lbowser')
                        i(class='fa fa-github-square fa-5x')
              .small-6.columns
                #mitch.front
                  img#zim(src='/media/zim2.png')
                  #content2.back.hidden 
                      a.links(href='https://github.com/MitchellSH')
                        i(class='fa fa-github-square fa-5x')
                      a.links(href='https://twitter.com/Mitch_Hollander')
                        i(class='fa fa-twitter-square fa-5x')
                      a.links(href='http://instagram.com/mitchell_hollander')
                        i(class='fa fa-instagram fa-5x')
                      a.links(href='https://plus.google.com/u/0/111891834390020142373/posts')
                        i(class='fa fa-google-plus-square fa-5x')
            .row
              .small-6.columns
                p#l Lauren Bowser
              .small-6.columns
                p#m Mitchell Hollander




      .row
        .small-6.columns
          #score-right
            p#right Right 
            p.rightAnswer
          #score-wrong
            p#wrong Wrong
            p.wrongAnswer
        .small-6.columns
          #container
            a#restart(href='#') Restart
            a#dropdown-quiz(href='#' data-dropdown='hover1' data-options='is_hover:true') Choose Quiz
            ul#hover1.f-dropdown(data-dropdown-content)
              li.0
                a(href='#').Q1 Quiz One
              li.1
                a(href='#').Q2 Quiz Two
              li.2
                a(href='#').Q3 Quiz Three
              li.3
                a(href='#').Q4 Quiz Four
              li.4
                a(href='#').Q5 Quiz Five
              li.5
                a(href='#').Q6 Quiz Six


      .row
        .small-6.columns
          #container2
            table#tableQuestions
              col.col1
              col.col2
              tr
                th#nameQuiz
                th Choice
              tr
                td
                    #q0
                td
                  .row
                    .small-3.columns
                        input(type='text').answer0
                    .small-3.columns
                        button.tiny.radius.submit1(data-qnum=0) submit
              tr
                td
                    #q1
                td
                  .row
                    .small-3.columns
                        input(type='text').answer1
                    .small-3.columns
                        button.tiny.radius.submit2(data-qnum=1) submit
              tr
                td
                    #q2
                td
                  .row
                    .small-3.columns
                        input(type='text').answer2
                    .small-3.columns
                        button.tiny.radius.submit3(data-qnum=2) submit
              tr
                td
                    #q3
                td
                  .row
                    .small-3.columns
                        input(type='text').answer3
                    .small-3.columns
                        button.tiny.radius.submit4(data-qnum=3) submit
              tr
                td
                    #q4
                td
                  .row
                    .small-3.columns
                        input(type='text').answer4
                    .small-3.columns
                        button.tiny.radius.submit4(data-qnum=4) submit

        .small-6.columns
          table#dov
             col.col1
             col.col2
             tr
               th#multipleChoice
               th Multiple Choice
             tr
               td.c0
             tr
               td.c1
             tr
               td.c2
             tr
               td.c3
             tr
               td.c4



  .row
    .small-6.columns
      #scroll-box
         h3#scroll-header Search Scroll
         #scroll-contents
           p.p hello
           p.p im
           p.p a
           p.p or
           p.p suppose
           p.p to 
           p.p be 
           p.p a 
           p.p scroll
           p.p box
           p.p and 
           p.p behold
           p.p I 
           p.p think 
           p.p this 
           p.p works
    .small-6.columns
      #search-div
        h3#search-title Search Box


  .row
    .small-12.columns
      #site-container
        .panel#site-links-header
          h1 Site Links
          p Reference Sources.
          #site-links


  .row
    .small-12.columns
      #about-page





script(src='/js/vendor/jquery.js')
script(src='/js/vendor/jqueryUI.js')
script(src='/js/vendor/jquery.flip.min.js')
script(src='/js/vendor/foundation.min.js')
script(src='/js/vendor/lodash.js')
script(src='/js/source/main.js')

