doctype html
//[if IE 9]><html class="lt-ie10" lang="en" > <![endif]
html(class='no-js', lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title Ecode
    link(rel='stylesheet', href='/css/normalize.css')
    link(rel='stylesheet', href='/css/foundation.css')
    link(rel='stylesheet', href='/css/app.css')
    script(src='/js/vendor/modernizr.js')

body

    .row
      .small-12.columns
        ul.breadcrumbs
          li.unavailable
            a(href='#')#ecode E_Code
          li 
            a(href='#')#home Home
          li
            a(href='#')#jquery Jquery
          li
            a(href='#')#html-jade HTML/Jade
          li
            a(href='#')#css CSS
          li
            a(href='#')#lodash LoDash
          li
            a(href='#')#git git

    .row
      .medium-12.columns
        .panel#welcome
          h1 Welcome To E_Code! 

    .row
      .small-12.columns
        #container
          button.tiny#start Start Quiz
          a.button.tiny(style='display: none')#search Search
          a.button.tiny(style='display: none')#quiz Quiz

    .row
      .small-12.columns
        #container2
          #container3
        table#tableQuestions
          col(style='width: 80%')
          col(style='width: 60%')
          tr
            th#nameQuiz
            th T or F
          tr
            td#q0
            td
              .row
                .small-6.columns
                  input(type='text').answer0
                .small-6.columns
                  button.tiny.radius.submit1(data-qnum=0) submit
          tr
            td#q1
            td
              .row
                .small-6.columns
                  input(type='text').answer1
                .small-6.columns
                  button.tiny.radius.submit2(data-qnum=1) submit
          tr
            td#q2
            td
              .row
                .small-6.columns
                  input(type='text').answer2
                .small-6.columns
                  button.tiny.radius.submit3(data-qnum=2) submit
          tr
            td#q3
            td
              .row
                .small-6.columns
                  input(type='text').answer3
                .small-6.columns
                  button.tiny.radius.submit4(data-qnum=3) submit
      .row
        .small-12.columns
          button.tiny(style='display: none')#again New Questions



    script(src='/js/vendor/jquery.js')
    script(src='/js/vendor/foundation.js')
    script(src='/js/vendor/lodash.js')
    script(src='/js/source/main.js')

